<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lims.api.approve.repository.ApprovalRepository">


    <insert id="insertApproval" parameterType="Approval" useGeneratedKeys="true" keyProperty="id">
        insert into approval(request_domain) values (#{requestDomain})
    </insert>


    <insert id="insertApprover" parameterType="Approver">
        insert into approver(
            approve_id,
            approver_name,
            approve_order,
            approve_yn
        )values (
            #{approveId},
            #{approverName},
            #{approveOrder},
            #{approveYn}
        )
    </insert>

</mapper>