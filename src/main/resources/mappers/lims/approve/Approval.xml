<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lims.api.approve.repository.ApprovalMapper">

    <insert id="insert" parameterType="Approval" useGeneratedKeys="true" keyProperty="id">
        insert into approval
            (
             approve_date,
            approval_status
            )
            values(
            #{approveDate},
            #{approvalStatus}
            )
    </insert>



</mapper>